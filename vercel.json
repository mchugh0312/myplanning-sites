{
  "rewrites": [
    {
      "source": "/:slug_group/:slug/:page",
      "destination": "/index.html"
    },
    {
      "source": "/:slug_group/:slug",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
